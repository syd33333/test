        System.out.println("Message ID: " + msgId);
        System.out.println("Creation DateTime: " + creDtTm);
        System.out.println("Settlement Method: " + settlementMethod);
        System.out.println("Clearing System: " + clearingSystem);
        System.out.println("Instruction ID: " + instrId);
        System.out.println("End-to-End ID: " + endToEndId);
        System.out.println("Transaction ID: " + txId);
        System.out.println("Amount: " + amount + " " + currency);
        System.out.println("Settlement Date: " + intrBkSttlmDt);
        System.out.println("Debtor Name: " + debtorName);
        System.out.println("Debtor Address: " + debtorAddress + ", Country: " + debtorCountry);
        System.out.println("Debtor IBAN: " + debtorIBAN);
        System.out.println("Creditor Name: " + creditorName);
        System.out.println("Creditor Address: " + creditorAddress + ", Country: " + creditorCountry);
        System.out.println("Creditor IBAN: " + creditorIBAN);
