        CustomerDirectDebitInitiationV02 cddi = mxPain008.getCstmrDrctDbtInitn();
        PaymentInformation3 pmtInf = cddi.getPmtInf().get(0);

        String creditorIBAN = pmtInf.getCdtrAcct().getId().getIBAN();
        String creditorBIC = pmtInf.getCdtrAgt().getFinInstnId().getBIC();
        String creditorOtherId = pmtInf.getCdtrAgt().getFinInstnId().getOthr().getId();

        DirectDebitTransactionInformation9 drctDbtTxInf = pmtInf.getDrctDbtTxInf().get(0);
        String debtorIBAN = drctDbtTxInf.getDbtrAcct().getId().getIBAN();
        String debtorBIC = drctDbtTxInf.getDbtrAgt().getFinInstnId().getBIC();
        String debtorOtherId = drctDbtTxInf.getDbtrAgt().getFinInstnId().getOthr().getId();
        String currency = drctDbtTxInf.getInstdAmt().getCcy();
   System.out.println("Creditor IBAN: " + creditorIBAN);
        System.out.println("Creditor BIC: " + creditorBIC);
        System.out.println("Creditor Other ID: " + creditorOtherId);
        System.out.println("Debtor IBAN: " + debtorIBAN);
        System.out.println("Debtor BIC: " + debtorBIC);
        System.out.println("Debtor Other ID: " + debtorOtherId);
        System.out.println("Currency: " + currency);
    }
