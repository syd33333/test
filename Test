        MxNode rootNode = MxNode.parse(fedwireXmlTemplate);
        MxNode creDtNode = rootNode.findFirstByName("CreDt", "urn:iso:std:iso:20022:tech:xsd:head.001.001.03");
        if (creDtNode != null) {
            creDtNode.setValue(currentTime); // Update CreDt with system time
        } else {
            System.err.println("No CreDt element found in the input XML");
            return;
        }
        String fedwireXml = rootNode.toXml(); // Serialize back to string
